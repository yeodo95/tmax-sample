CREATE database root1;

CREATE USER 'root1'@'%' IDENTIFIED BY 'tmax@23';
CREATE USER 'root1'@'localhost' IDENTIFIED BY 'tmax@23';

GRANT ALL PRIVILEGES ON root1.* TO 'root1'@'localhost';                            
GRANT ALL PRIVILEGES ON root1.* TO 'root1'@'%';



CREATE TABLE root1.ZIPCODE  (
id  VARCHAR(50) NULL  ,
inpt_dt  DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP ,
ZIP_NO  VARCHAR(5) NULL ,
SIDO  VARCHAR(20) NULL  ,
SIDO_ENG  VARCHAR(40) NULL  ,
SIGUNGU  VARCHAR(20) NULL ,
SIGUNGU_ENG  VARCHAR(40),
EUPMYUN  VARCHAR(20) NULL  ,
EUPMYUN_ENG  VARCHAR(40) NULL ,
DORO_CD  VARCHAR(12) NULL ,
DORO  VARCHAR(80) NULL  ,
DORO_ENG  VARCHAR(80) NULL ,
UNDERGROUND_YN  CHAR(1) NULL ,
BUILD_NO1  DECIMAL(5,0) NULL ,
BUILD_NO2  DECIMAL(5,0) NULL ,
BUILD_NO_MANAGE_NO  VARCHAR(25) ,
DARYANG_NM  VARCHAR(40) NULL  ,
BUILD_NM  VARCHAR(200) NULL  ,
DONG_CD  VARCHAR(10) NULL  ,
DONG_NM  VARCHAR(20) NULL  ,
RI  VARCHAR(20) NULL  ,
H_DONG_NM  VARCHAR(40) NULL  ,
SAN_YN  VARCHAR(1) NULL  ,
ZIBUN1  DECIMAL(4,0) NULL ,
EUPMYUN_DONG_SN  VARCHAR(2)  ,
ZIBUN2  DECIMAL(4,0) NULL ,
ZIP_NO_OLD  VARCHAR(4) NULL  ,
ZIP_SN  VARCHAR(2) NULL
) ENGINE = InnoDB ROW_FORMAT = DEFAULT CHARACTER SET utf8;

 

create table root1.INS_TT
(
id varchar(50),
inpt_dt DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP
);


